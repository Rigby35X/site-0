		{
			"src": "^/api/client-data/?$",
			"dest": "_render"
		},
		{
			"src": "^/applications/?$",
			"dest": "_render"
		},
		{
			"src": "^/contact/?$",
			"dest": "_render"
		},
		{
			"src": "^/donate/?$",
			"dest": "_render"
		},
		{
			"src": "^/events/?$",
			"dest": "_render"
		},
		{
			"src": "^/our-animals/?$",
			"dest": "_render"
		},
		{
			"src": "^/$",
			"dest": "_render"
		}
	]
}