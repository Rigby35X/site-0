			"dest": "_render"
		},
		{
			"src": "^/api/admin/animals/bulk-import/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/animals/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/generate-social-content/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/organization/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/test-openai/?$",
			"dest": "_render"
		},
		{
			"src": "^/api/admin/update-client-data/?$",
			"dest": "_render"
